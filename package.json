{
  "name": "@reducto/services",
  "version": "0.1.0",
  "description": "A collection of higher order middleware for making service calls.",
  "main": "index.js",
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "lint": "semistandard",
    "mocha": "NODE_ENV=test mocha -R dot",
    "test": "npm run mocha",
    "pretest": "npm run lint",
    "test:watch": "npm run mocha -- --watch"
  },
  "keywords": [
    "express",
    "api",
    "services",
    "async",
    "batch"
  ],
  "author": "Michael Allen",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/michaelleeallen/reducto-services.git"
  },
  "bugs": {
    "url": "https://github.com/michaelleeallen/reducto-services/issues"
  },
  "homepage": "https://github.com/michaelleeallen/reducto-services",
  "devDependencies": {
    "mocha": "^3.1.2",
    "nock": "^8.1.0",
    "semistandard": "^9.1.0",
    "testdouble": "^1.8.0"
  },
  "dependencies": {
    "lodash": "^4.16.4",
    "request": "^2.75.0"
  },
  "semistandard": {
    "env": {
      "node": true,
      "mocha": true,
      "es6": true
    },
    "ignore": [
      "test/"
    ]
  }
}
